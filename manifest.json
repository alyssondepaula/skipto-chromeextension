{
  "manifest_version": 2,
  "name": "Skip to Movie",
  "version": "1.0",
  "description": "Skip!",
  "browser_action": {
    "default_title": "Skip to Movie",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}
